public class ExecutingClass {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		EmployeeClass mylist[] = {new EmployeeClass(1001,"Raj",3209.98),
									new EmployeeClass(1010,"Shiv",9980),
									new EmployeeClass(990,"Rina",44044.10),
									new EmployeeClass(1099,"Lakhan",1000.50)};
		
		for(EmployeeClass element:mylist){
			element.PrintEmployee();
		}
	}
}

class GenericSorter<T> {
	//Insertion sort to sort employees by their employee id
	public void SortMethod(Comparable <T> ListParameter[]){
		for (int i=0;i<ListParameter.length;i++){
			Comparable currentObject = ListParameter[i];
			
		}
	}
}



public class EmployeeClass implements Comparable <EmployeeClass>{
	private int EmpID;
	private String Name;
	private double Salary;
	
	//Constructor to initialize the object with parameter values
	
	EmployeeClass(int a, String b, double c){
		this.EmpID = a;
		this.Name = b;
		this.Salary=c;
	}
	public int compareTo(EmployeeClass EmpObj) {
		// TODO Auto-generated method stub
		if (this.EmpID>EmpObj.EmpID){return 1;}
		else if (this.EmpID<EmpObj.EmpID){return -1;}
		else {return 0;}
	}
	
	public void PrintEmployee(){
		System.out.println("EmpID: "+this.EmpID+", Name: "+this.Name+", Salary: "+this.Salary);
	}
}
